apiVersion: kubectl.config.k8s.io/v1beta1
kind: Preference
defaults:
- command: delete
  options:
  - name: interactive
    default: "true"
- command: apply
  options:
  - name: server-side
    default: "true"

aliases:
- name: gy
  command: get
  options:
  - name: output
    default: yaml
- name: gj
  command: get
  options:
  - name: output
    default: json
- name: gw
  command: get
  options:
  - name: output
    default: wide
